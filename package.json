{
    "name": "pipeparatestes",
    "version": "1.0.0",
    "main": "app.js",
    "scripts": {
      "dev": "cross-env NODE_ENV=development nodemon app.js",
      "prd": "cross-env NODE_ENV=production node app.js",
      "build": "cross-env NODE_ENV=production gulp default",
      "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha 'app/__test__/unitTests/*.test.js'",
      "autotest": "cross-env NODE_ENV=test nyc --reporter=lcov mocha 'app/__test__/integrationTests/*.test.js'"
    },
    "mocha": {
      "diff": true,
      "extension": [
        "js",
        "cjs",
        "mjs"
      ],
      "package": "./package.json",
      "reporter": "spec",
      "slow": "75",
      "ui": "bdd",
      "watch-files": [
        "lib/**/*.js",
        "app/__test__/**/*.js"
      ],
      "watch-ignore": [
        "lib/vendor"
      ]
    },
    "nodemonConfig": {
      "ignore": [
        "**/__test__/**",
        "**/docs/**",
        "**/content/**",
        "*.json"
      ]
    },
    "keywords": [],
    "author": "Claro",
    "license": "ISC",
    "dependencies": {
      "body-parser": "^1.20.2",
      "cross-env": "^7.0.3",
      "ejs": "^3.1.10",
      "express": "^4.19.2",
      "nyc": "^15.1.0",
      "serve-favicon": "^2.5.0"
    },
    "devDependencies": {
      "chai": "^4.4.1",
      "chai-dom": "^1.12.0",
      "chai-fetch": "^0.3.1",
      "chai-http": "^4.4.0",
      "chai-param": "^0.1.2",
      "eslint": "^8.57.0",
      "eslint-config-standard": "^17.1.0",
      "eslint-plugin-import": "^2.29.1",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^6.1.1",
      "gulp": "^5.0.0",
      "gulp-clean-css": "^4.3.0",
      "gulp-concat": "^2.6.1",
      "gulp-uglify": "^3.0.2",
      "jsdom": "^24.0.0",
      "jsdom-global": "^3.0.2",
      "mocha": "^10.4.0",
      "mocha-sinon": "^2.1.2",
      "mocked-env": "^1.3.5",
      "mockttp": "^3.10.2",
      "nodemon": "^3.1.0",
      "sinon": "^17.0.1",
      "sinon-chai": "^3.7.0"
    },
    "repository": {
      "type": "",
      "url": ""
    },
    "description": ""
  }